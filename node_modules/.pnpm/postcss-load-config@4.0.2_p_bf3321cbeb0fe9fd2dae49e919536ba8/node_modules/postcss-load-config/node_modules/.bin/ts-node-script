#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules/ts-node/dist/node_modules:/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules/ts-node/node_modules:/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules:/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules/ts-node/dist/node_modules:/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules/ts-node/node_modules:/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules:/proc/cygdrive/d/Quorium-internship/hotel-management-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules/ts-node/dist/bin-script.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_@swc+core@1._a50fca3f25e15f69a18922447307f7e9/node_modules/ts-node/dist/bin-script.js" "$@"
fi
